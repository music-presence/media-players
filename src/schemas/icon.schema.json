{
  "type": "object",
  "additionalProperties": false,
  "required": [
    "label",
    "type",
    "url"
  ],
  "properties": {
    "label": {
      "type": "string",
      "pattern": "^[a-z][a-z\\-]*[a-z]$"
    },
    "type": {
      "type": "string",
      "enum": [
        "png",
        "jpg",
        "ico"
      ]
    },
    "url": {
      "type": "string",
      "format": "uri",
      "pattern": "^https://\\S+[^/\\s]$"
    },
    "md5": {
      "$comment": "MD5 hash of the file at the url",
      "type": "string",
      "pattern": "^[0-9a-f]{32}$"
    }
  }
}